@model GeradorDeRotas.Models.Equipe

@{
	ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
	<h4>Equipe</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.NomeEquipe)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.NomeEquipe)
		</dd>
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Cidade)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Cidade)
		</dd>
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.Servico)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Servico)
		</dd>
		<dt class="col-sm-12">
			@Html.DisplayNameFor(model => model.Pessoas)
		</dt>
		@foreach (var item in Model.Pessoas)
		{
			<dd class="col-sm-6">
				@Html.DisplayFor(modelItem => item.Nome);
			</dd>
		}
	</dl>
</div>
<div>
	<a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
	<a asp-action="Index">Voltar</a>
</div>
